use IO;
// use Time;

// var t: Timer;
// var rt: Timer;
// var wt: Timer;
// t.start();
// rt.start();
var myFile = open("input", iomode.cw);
var myWritingChannel = myFile.writer();
var x: int = 10;
myWritingChannel.write(x);
myWritingChannel.flush();
myFile = open("input", iomode.r);
var myReadingChannel = myFile.reader();
var y: int;
myReadingChannel.read(y);
writeln(y);
myWritingChannel.close();
myReadingChannel.close();
myFile.close();

var k: int;
readf("%i", k);
var a: int = k.stringStyleWithVariableLength();
writeln(a);
var b: int = stringStyleExactLen(6);
writeln(b);
